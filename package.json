{
  "name": "demuxer",
  "version": "2.1.2",
  "description": "A utility for transmux ts/mp4/flv",
  "main": "dist/demuxer.all.cjs.js",
  "module": "dist/demuxer.all.esm.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -c ./rollup.config.js -w -m --environment BUILD_MODE:dev",
    "production": "rollup -c ./rollup.config.js --environment BUILD_MODE:production",
    "lint": "eslint --ext js,ts src",
    "test": "karma start",
    "doc": "jsdoc ./src/ -c doc/jsdoc.json"
  },
  "author": "gems.xu@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^7.4.0",
    "@commitlint/config-conventional": "^7.3.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@types/jasmine": "^3.5.10",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "commitizen": "^3.0.5",
    "eslint": "^6.1.0",
    "git-cz": "^3.2.1",
    "husky": "^1.3.1",
    "jasmine-core": "^3.4.0",
    "jsdoc": "^3.5.5",
    "karma": "^5.0.9",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-rollup": "^1.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^5.0.2",
    "lint-staged": "^8.1.4",
    "prettier": "2.0.5",
    "rollup": "^2.8.2",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goldvideo/demuxer.git"
  },
  "keywords": [
    "demux",
    "typescript",
    "streaming data",
    "mpegts",
    "ts",
    "mp4",
    "flv"
  ],
  "bugs": {
    "url": "https://github.com/goldvideo/demuxer/issues"
  },
  "homepage": "https://github.com/goldvideo/demuxer#readme",
  "sideEffects": false
}
